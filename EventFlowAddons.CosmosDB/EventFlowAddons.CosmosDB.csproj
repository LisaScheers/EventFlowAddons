<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>netstandard2.1;netcoreapp3.1;net6.0</TargetFrameworks>
        <GenerateAssemblyInfo>true</GenerateAssemblyInfo>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>LisaScheers.EventFlowAddons.CosmosDB</Title>
        <Authors>Lisa Scheers</Authors>
        <Copyright>Lisa Scheers</Copyright>
        <PackageProjectUrl>https://github.com/LisaScheers/EventFlowAddons</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/LisaScheers/EventFlowAddons/blob/main/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/LisaScheers/EventFlowAddons</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <AssemblyName>LisaScheers.EventFlowAddons.CosmosDB</AssemblyName>
        <RootNamespace>LisaScheers.EventFlowAddons.CosmosDB</RootNamespace>
        <PackageId>LisaScheers.EventFlowAddons.CosmosDB</PackageId>
        <Product>LisaScheers.EventFlowAddons.CosmosDB</Product>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="EventFlow" Version="1.0.5002-alpha" />
        <PackageReference Include="Microsoft.Azure.Cosmos" Version="3.32.1" />
    </ItemGroup>

</Project>
